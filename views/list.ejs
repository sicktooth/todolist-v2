<%- include('header'); -%>
        <main class="mx-auto my-12 max-w-sm">
                <header class="text-center mb-12">
                    <h1 class="bg-purple-300 p-2 text-white text-3xl font-bold" id="head"><%= listTitle %></h1>
                </header>
                <section class="bg-white box">
                    <ul>
                    <% for (let start = 0; start<newListItems.length; start++){ %>
                        <li class="item"><input type="checkbox" class="checkBox m-5 cursor-pointer"> <%= newListItems[start] %></li>
                    <% } %>
                    </ul>
                    <form action="/" method="post" class="form">
                        <input type="text" name="newItem" placeholder="New Item">
                        <button type="submit" class="btn" name="list" value=<%= listTitle %>>
                            +
                        </button>
                    </form>
                </section>
            
<%- include('footer'); -%>